  <DataSets>
    <DataSet Name="dsListaBasesDatos">
      <Query>
        <DataSourceName>Servidor</DataSourceName>
        <CommandText>-- Lista Bases de datos del server.
SELECT IDBaseDatos = DB.DATABASE_ID
	 , BaseDatos   = DB.NAME
  FROM MASTER.SYS.DATABASES DB
 WHERE DB.NAME NOT IN ('master', 'tempdb', 'model', 'msdb')
 ORDER BY DB.NAME;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="IDBaseDatos">
          <DataField>IDBaseDatos</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="BaseDatos">
          <DataField>BaseDatos</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
