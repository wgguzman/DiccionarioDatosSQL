        <Field Name="IDEsquema">
          <DataField>IDEsquema</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Esquema">
          <DataField>Esquema</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IDVista">
